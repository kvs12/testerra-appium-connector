<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- Suite to run for QCSync3 integration tests. -->
<suite name="Systemtest" verbose="10" configfailurepolicy="continue" parallel="false">

    <listeners>
        <listener class-name="eu.tsystems.mms.tic.testframework.report.TesterraListener"/>
    </listeners>

    <test name="DeviceCheck" parallel="false">
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.mobile.systemtest.deviceCheck.DeviceCheck"></class>
        </classes>
    </test>


</suite>
